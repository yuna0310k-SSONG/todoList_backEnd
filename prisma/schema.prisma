generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model todos {
  id          Int       @id @default(autoincrement())
  title       String
  is_done     Boolean   @default(false)
  target_date DateTime?
  created_at  DateTime  @default(now())
}
